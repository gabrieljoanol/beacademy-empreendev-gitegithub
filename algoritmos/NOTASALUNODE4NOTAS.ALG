Algoritmo "NotasAlunode4notas"

var nota1, nota2, nota3 , nota4: real
inicio
      escreva("digite a 1º nota")
      leia (nota1)
      escreva("digite a 2º nota")
      leia (nota2)
      escreva("digite a 3º nota")
      leia (nota3)
      escreva("digite a 4º nota")
      leia (nota4)
      escreva("media obtida" , ((((nota1+nota2+nota3+nota4)))/4))
       Fimalgoritmo